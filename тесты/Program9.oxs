namespace ConsoleApplication;
interface
Program9 = class
public
class method main();
class method factorial(int n): Integer;
end;
implementation
class method Program9.factorial(int n): Integer;
begin
if (n == 1)
begin
return 1;
end;
int t = Program9.factorial(n-1);
int result = n * t;
return result;
end;
class method Program9.main();
begin
int r = Program9.factorial(5);
print(r);
end;
end.